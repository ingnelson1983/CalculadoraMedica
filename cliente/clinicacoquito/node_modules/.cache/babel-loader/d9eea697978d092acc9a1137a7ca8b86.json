IAAA,CAAA,MAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,WAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,KAAA,GAAA,SAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,KAAA,GAAA,eAAA;;AACA,QAAA,MAAA,CAAA,SAAA;AAGA,OAXA,EAYA,KAZA,CAYA,UAAA,CAAA,EAAA;AAEA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA,QAAA;AAEA,OAhBA;AAmBA,KAxCA;AA0CA,IAAA,YA1CA,wBA0CA,EA1CA,EA0CA;AAAA;;AAEA,WAAA,KAAA,CAAA,MAAA,iBAAA,EAAA,GACA,IADA,CACA,UAAA,GAAA,EAAA;AAEA,YAAA,KAAA,GAAA,MAAA,CAAA,KAAA,CAAA,SAAA,CAAA,UAAA,IAAA;AAAA,iBAAA,IAAA,CAAA,GAAA,KAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AAAA,SAAA,CAAA;;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,KAAA,GAAA,SAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,KAAA,GAAA,gBAAA;;AACA,QAAA,MAAA,CAAA,SAAA;AAGA,OAVA,EAWA,KAXA,CAWA,UAAA,CAAA,EAAA;AAEA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA,QAAA;AAEA,OAfA;AAgBA,KA5DA;AA8DA,IAAA,cA9DA,0BA8DA,EA9DA,EA8DA;AAAA;;AAEA,WAAA,MAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,GAAA,iBAAA,EAAA,GACA,IADA,CACA,UAAA,GAAA,EAAA;AAEA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA;AAEA,OALA,EAMA,KANA,CAMA,UAAA,CAAA,EAAA;AAEA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA,QAAA;AAGA,OAXA;AAcA,KA/EA;AAiFA,IAAA,UAjFA,sBAiFA,IAjFA,EAiFA;AAAA;;AAEA,WAAA,KAAA,CAAA,GAAA,iBAAA,IAAA,CAAA,GAAA,GAAA,IAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,KAAA,GAAA,MAAA,CAAA,KAAA,CAAA,SAAA,CAAA,UAAA,CAAA;AAAA,iBAAA,CAAA,CAAA,GAAA,KAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AAAA,SAAA,CAAA;;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,KAAA,EAAA,MAAA,GAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,KAAA,EAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,WAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,KAAA,GAAA,SAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,KAAA,GAAA,cAAA;;AACA,QAAA,MAAA,CAAA,SAAA;;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AAGA,OAXA,EAYA,KAZA,CAYA,UAAA,CAAA,EAAA;AAEA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA,QAAA;AAEA,OAhBA;AAoBA,KAvGA;AAwGA,IAAA,gBAxGA,4BAwGA,gBAxGA,EAwGA;AACA,WAAA,gBAAA,GAAA,gBAAA;AACA,KA1GA;AA2GA,IAAA,SA3GA,uBA2GA;AACA,WAAA,gBAAA,GAAA,KAAA,WAAA;AACA;AA7GA;AAxBA,CAAA","sourcesContent":["<template>\r\n<div class=\"container\">\r\n    <h1>Pacientes</h1>\r\n\r\n    <b-alert \r\n    :show=\"dismissCountDown\" \r\n    dismissible \r\n    :variant=\"mensaje.color\" \r\n    @dismissed=\"dismissCountDown=0\" \r\n    @dismiss-count-down=\"countDownChanged\" > \r\n    {{mensaje.texto}} \r\n    </b-alert>\r\n\r\n\r\n    <form @submit.prevent=\"editarNota(notaEditar)\" v-if=\"editar\">\r\n        <h3>Editar nota</h3>\r\n\r\n        <input type=\"text\" class=\"form-control my-2\" placeholder=\"Nombre\" v-model=\"notaEditar.nombre\">\r\n        <input type=\"text\" class=\"form-control my-2\" placeholder=\"Descripcion\" v-model=\"notaEditar.descripcion\">\r\n        <b-button class=\"btn-success my-2 mx-2\" type=\"submit\">Editar</b-button>\r\n        <b-button class=\" my-2\" type=\"submit\" @click=\"editar=false\">Cancelar</b-button>\r\n\r\n    </form>\r\n    <form @submit.prevent=\"agregarNota()\" v-if=\"!editar\">\r\n        <h3>Agregar una nueva nota</h3>\r\n\r\n        <input type=\"text\" class=\"form-control my-2\" placeholder=\"Nombre\" v-model=\"nota.nombre\">\r\n        <input type=\"text\" class=\"form-control my-2\" placeholder=\"Descripcion\" v-model=\"nota.descripcion\">\r\n        <b-button class=\"btn-success my-2\" type=\"submit\">Agregar</b-button>\r\n\r\n    </form>\r\n\r\n    <table class=\"table\">\r\n        <thead>\r\n            <tr>\r\n                <th scope=\"col\">#</th>\r\n                <th scope=\"col\">Nota</th>\r\n                <th scope=\"col\">Descripcion</th>\r\n                <th scope=\"col\">Acciones</th>\r\n            </tr>\r\n        </thead>\r\n        <tbody>\r\n            <tr v-for=\"(item, index) in notas\" :key=\"index\">\r\n                <th scope=\"row\">{{item._id}}</th>\r\n                <td>{{item.nombre}}</td>\r\n                <td>{{item.descripcion}}</td>\r\n                <td>\r\n                    <b-button class=\"btn-danger mx-2\" @click=\"eliminarNota(item._id)\">Eliminar</b-button>\r\n                    <b-button class=\"btn-warning mx-2\" @click=\"activarEdicion(item._id)\">Editar</b-button>\r\n                </td>\r\n            </tr>\r\n\r\n        </tbody>\r\n    </table>\r\n\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n\r\n    data() {\r\n        return {\r\n\r\n            notas: [],\r\n            mensaje: {color: 'success', texto: ''}, \r\n            dismissSecs: 5, \r\n            dismissCountDown: 0,\r\n\r\n            nota:{nombre:\"\",descripcion:\"\"},\r\n            editar:false,\r\n            notaEditar:{}\r\n\r\n        }\r\n\r\n    },\r\n\r\n    created() {\r\n\r\n        this.listarNotas();\r\n\r\n    },\r\n\r\n    methods: {\r\n\r\n        listarNotas() {\r\n\r\n            this.axios.get('/nota')\r\n                .then(res => {\r\n                    console.log(res.data);\r\n                    this.notas = res.data;\r\n\r\n                })\r\n                .catch(e => {\r\n\r\n                    console.log(e.response);\r\n\r\n                })\r\n\r\n        },\r\n\r\n        agregarNota(){\r\n\r\n\r\n            this.axios.post('/nueva-nota',this.nota)\r\n            .then(res=>{\r\n\r\n                this.notas.push(res.data)\r\n                this.nota.nombre=\"\";\r\n                this.nota.descripcion=\"\";\r\n                this.mensaje.color=\"success\";\r\n                this.mensaje.texto=\"Nota Agregada\";\r\n                this.showAlert();\r\n\r\n\r\n            })\r\n            .catch(e=>{\r\n\r\n                console.log(e.response);\r\n\r\n            })\r\n\r\n\r\n        },\r\n\r\n        eliminarNota(id){\r\n\r\n            this.axios.delete(`/nota/${id}`)\r\n            .then(res=>{\r\n\r\n                const index = this.notas.findIndex(item=> item._id===res.data._id);\r\n                this.notas.splice(index, 1);\r\n                this.mensaje.color=\"success\";\r\n                this.mensaje.texto=\"Nota Eliminada\";\r\n                this.showAlert();\r\n\r\n\r\n            })\r\n            .catch(e=>{\r\n\r\n                  console.log(e.response);\r\n\r\n            })\r\n        },\r\n\r\n        activarEdicion(id){\r\n\r\n            this.editar=true;\r\n            this.axios.get(`/nota/${id}`)\r\n            .then(res=>{\r\n\r\n                this.notaEditar=res.data;\r\n\r\n            })\r\n            .catch(e=>{\r\n\r\n                 console.log(e.response);\r\n\r\n\r\n            })\r\n\r\n\r\n        },\r\n\r\n        editarNota(item){\r\n            \r\n            this.axios.put(`/nota/${item._id}`, item)\r\n            .then(res=>{\r\n                const index= this.notas.findIndex(n=> n._id===res.data._id);\r\n                this.notas[index].nombre=res.data.nombre;\r\n                this.notas[index].descripcion=res.data.descripcion;\r\n                this.mensaje.color=\"success\";\r\n                this.mensaje.texto=\"Nota Editada\";\r\n                this.showAlert();\r\n                this.editar=false;\r\n\r\n\r\n            })\r\n            .catch(e=>{\r\n\r\n                console.log(e.response);\r\n\r\n            })\r\n\r\n\r\n\r\n        },\r\n        countDownChanged(dismissCountDown) { \r\n            this.dismissCountDown = dismissCountDown \r\n        }, \r\n        showAlert() { \r\n            this.dismissCountDown = this.dismissSecs \r\n        }\r\n\r\n    }\r\n\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views"}]}